{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= answers.description %>",
  "author": "<%- answers.author %>",
  "scripts": {
    "dev": "APP_MODE=dev webpack-dev-server --config build/webpack.dev.config.js",
    "build:SIT": "APP_MODE=sit webpack --config build/webpack.prod.config.js",
    "build:UAT": "APP_MODE=uat webpack --config build/webpack.prod.config.js",
    "build:PRE": "APP_MODE=pre webpack --config build/webpack.prod.config.js",
    "build:PROD": "APP_MODE=prod webpack --config build/webpack.prod.config.js",
    "postinstall": "npx husky install",
    "setHusky": "npx husky set .husky/pre-commit 'npx lint-staged' && npx husky set .husky/commit-msg 'npx --no-install commitlint --edit $1'"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  },
  "dependencies": {
    "core-js": "^3.33.2",
    "lodash.throttle": "^4.1.1",
    "swagger": "github:thjjames/swagger",
    "vue": "^2.6.10",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.0.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@vue/babel-preset-app": "^5.0.8",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^14.0.1",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.0.2",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.7.10",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
