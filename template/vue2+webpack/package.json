{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= answers.description %>",
  "author": "<%- answers.author %>",
  "main": "./src/main.ts",
  "scripts": {
    "dev": "APP_MODE=dev webpack-dev-server --config build/webpack.dev.config.js",
    "build:SIT": "APP_MODE=sit webpack --config build/webpack.prod.config.js",
    "build:UAT": "APP_MODE=uat webpack --config build/webpack.prod.config.js",
    "build:PRE": "APP_MODE=pre webpack --config build/webpack.prod.config.js",
    "build:PROD": "APP_MODE=prod webpack --config build/webpack.prod.config.js",
    "postinstall": "npx husky install",
    "setHusky": "npx husky set .husky/pre-commit 'npx lint-staged' && npx husky set .husky/commit-msg 'npx --no-install commitlint --edit $1'"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  },
  "dependencies": {
    "dayjs": "^1.10.8",
    "lodash.throttle": "^4.1.1",
    "swagger": "github:thjjames/swagger",
    "vue-property-decorator": "^9.1.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-syntax-jsx": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.8",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime-corejs3": "^7.15.8",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vue/babel-preset-app": "^4.5.9",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.17.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^7.0.2",
    "less": "^3.10.0",
    "less-loader": "^6.2.0",
    "lint-staged": "^11.1.2",
    "portfinder": "^1.0.28",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^1.0.0",
    "url-loader": "^1.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.10",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.0",
